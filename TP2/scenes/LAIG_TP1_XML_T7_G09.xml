          <rotation axis="y" angle="-90.0" />
        </transformations>
        <descendants>
          <leaf type="rectangle" x1="0.0" y1="0.0" x2="20.0" y2="5.0" />
        </descendants>
      </node>

      <node id="parede_esquerda">
